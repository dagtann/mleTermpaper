@misc{Mller.22.27..03.2010,
 abstract = {Few contemporary attempts to lay bare the causes of autocracy can be identified in the literature. In this article, we venture down that road, in turn assessing whether structural factors that have been highlighted as necessary for democracy are sufficient for autocracy in the context of the so-called third wave of democratization. Based on current writings on democratization, one would not expect this to be case. What is more, the tendency of the structural factors to be sufficient should be even less pronounced following the breakdown of communism. However, using fuzzy-set QCA on data covering the period 1972-2008, we show that neither of these expectations survives an empirical reality check. In gist, a number of sufficient conditions of autocracy can in fact be indentified and -- counter-intuitively -- the extent to which a number of them condition autocracy has increased, not decreased, over the period. We provide some tentative hypotheses to make sense of these findings.},
 author = {M{\o}ller, J{\o}rgen and Skaaning, Svend-Erik},
 year = {22.-27..03.2010},
 title = {{C}onditions of {A}utocracy},
 address = {M{\"u}nster},
 series = {{E}{C}{P}{R}~{J}oint {S}essions of {W}orkshops}
}


@article{Moe.2005,
 abstract = {Rational choice theory tends to viewpolitical institutions as structures of voluntary cooperation that resolve collective action problems and benefit all concerned. Yet the political process often gives rise to institutions that are good for some people and bad for others, depending on who has the power to impose their will. Political institutions may be structures of cooperation, but they may also be structures of power---and the theory does not tell us much about this. As a result, it gives us a one-sided and overly benign view of what political institutions are and do. This problem is not well understood, and indeed is not typically seen as a problem at all. For there is a widespread sense in the rational choice literature that, because power is frequently discussed, it is an integral part of the theory and just as fundamental as cooperation. Confusion on this score has undermined efforts to right the imbalance.My purpose here is to clarify the analytic roles that power and cooperation actually play in this literature, and to argue that a more balanced theory---one that brings power fromits periphery to its very core---is both necessary and entirely possible.},
 author = {Moe, Terry M.},
 year = {2005},
 title = {{P}ower and {P}olitical {I}nstitutions},
 urldate = {27.09.2011},
 pages = {215--233},
 volume = {3},
 number = {2},
 journal = {Perspectives on Politics}
}


