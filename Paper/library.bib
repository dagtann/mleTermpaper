@misc{Tanneberg.2011c,
 abstract = {This discussion paper, first, is meant to disentangle conceptually the (inconclusive) discussions about the rivalling concepts of stability,
persistence, and change. It claims a necessity to focus on phenomena
of persistence, rather than stability, in any empirical causal analysis.
Inferences on stability can only be made after the causal relationships
of the three pillars and persistence have been established.
Second, we present an operationalization of three instances of change
in autocratic regimes: change of the ruler; change of the autocratic
subtype; and the transition of autocracy to democracy.
Third, we introduce a new measure of autocratic persistence,
combining the three individual levels.
Finally, we offer some tentative descriptive inference of the empirics of
change in autocracies.},
 author = {Tanneberg, Dag and Schmotz, Alexander},
 year = {2011},
 title = {{T}he {S}urvival of {A}utocracies: {S}tability, {P}ersistence, and {C}hange}
}


