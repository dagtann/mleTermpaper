@misc{Jensen.08.12.2010,
 abstract = {Recent scholarship on the relationship between institutions and foreign direct investment has largely focused on the differences between democratic and autocratic regimes. While some recent studies have examined how electoral rules affect multinational investors, scant attention has been paid to the tremendous variation within autocratic regimes. In this paper we explore the theoretical argument that institutions within autocratic regimes, specifically the existence of legislatures with more than one political party, can reduce levels of political risk for multinational investors. To test the relationship between authoritarian institutions and property rights protection we utilize three different empirical tests. First, we describe the results of an original online survey of political risk insurers, plant location consultants, and risk analysts. Our survey indicates that variations in authoritarian regimes, including elected legislatures, have very little impact on property rights protections. Second, using a database from the political risk insurance industry, we find that competitive legislatures have no meaningful impact on political risk. Third, using firm-level data from the World Bank Enterprise Surveys, we find that political parties and legislatures are associated with a weaker property rights regime, and higher levels of economic and regulatory policy uncertainty. Our findings cast considerable doubt that the key theoretical link between authoritarian institutions and economic growth operates through the protection of property rights.},
 author = {Jensen, Nathan and Malesky, Edmund and Weymouth, Stephen},
 year = {08.12.2010},
 title = {{B}inding the {G}rabbing {H}and?~{L}egislatures and {E}xpropriation {R}isk in {A}uthoritarian {R}egimes},
 urldate = {07.12.2011}
}


