@Book{James.2013,
  Title                    = {{A}n introduction to statistical learning: {W}ith applications in {R}},
  Author                   = {James, Gareth and Witten, Daniela and Hastie, Trevor and Tibshirani, Robert},
  Publisher                = {Springer},
