@article{Bernhard.2003b,
 abstract = {In the literature on democratic survival, theories of democratic consolidation assume that new democracies are more vulnerable to breakdown. Theories of democratic honeymoons, however, claim that newdemocracies are less vulnerable to breakdown. This article addresses this seeming contradiction. Using an original data set that includes all democracies from the period 1951 to 1995, the authors use discrete-time duration analysis to determine if there is evidence for a period of enhanced survivability in newdemocracies. Using both continuous and discrete specifications of a honeymoon period, they test whether new democracies experience an absolute honeymoon(whether newness in itselfmakes them less prone to breakdown) or a relative honeymoon (where newness insulates them from the effect of poor economic performance). The results suggest that there is a short-lived absolute honeymoon, but that new democracies are actually more vulnerable to effects of poor economic performance prior to their third legislative election.},
 author = {Bernhard, Michael and Reenock, Christopher and Nordstrom, Timothy},
 year = {2003},
 title = {{E}conomic {P}erformance and {S}urvival in {N}ew {D}emocracies: {I}s {T}here a {H}oneymoon {E}ffect?},
 keywords = {breakdown;democratization;economic growth;honeymoon;political economy},
 pages = {404--431},
 volume = {36},
 number = {4},
 journal = {Comparative Political Studies}
}


@article{Bernhard.2004,
 abstract = {Using an original dataset that covers the period from 1951 to 1995, we
consider the enduring effects of Western overseas colonialism on the
democratic survival of postcolonial democracies. We treat colonialism as
a holistic phenomenon and differentiate the relative effects of its legacies
with regard to the level of economic development, social fragmentation,
and the relationship between the state and civil society. We find that
Western overseas colonialism, a factor often overlooked in recent large-n
studies, continues to have an effect on the survival of democratic
regimes. We further find that the legacy of specific colonial powers has
an important effect on survival as well. Unlike previous studies, we find
that former Spanish colonies outperform British colonies when
colonialism is conceptualized holistically. However, when we break
colonial legacy into separate components (development, social fragmentation, and the relationship between the state and civil society), we
find that the advantages former British colonies enjoy are attributable to
the legacy of the state/civil society relationship. Moreover, we show that
at least in the case of former British colonies, time spent under colonial
rule is positively associated with democratic survival.},
 author = {Bernhard, Michael and Reenock, Christopher and Nordstrom, Timothy},
 year = {2004},
 title = {{T}he {L}egacy of {W}estern {O}verseas {C}olonialism on {D}emocratic {S}urvival},
 keywords = {autocratization;quantitative research;survival analysis;colonialism;post-colonialism;fractionalization},
 pages = {225--250},
 volume = {48},
 number = {1},
 journal = {International Studies Quarterly}
}


@article{Bernholz.1991,
 abstract = {It is the purpose of the present paper to show that totalitarianism constitutes a rational system which binds population and leaders by stable long-lasting rules, which form an unwritten or written constitution. It is exactly this constitutional trait of totalitarianism which is one of the main differences distinguishing it from dictatorship or tyranny, since the latter are characterized by the uncontrolled discretionary power of the tyrant. Finally, necessary elements of the Constitution of Totalitarianism are identified. Es ist das Ziel des vorliegenden Beitrages zu zeigen, da{\ss} es sich beim Totalitarismus um ein rationales System handelt, das die Bev{\"o}lkerung und die F{\"u}hrer durch stabile langfristig g{\"u}ltige Regeln bindet, die wiederum eine geschriebene oder ungeschriebene Verfassung repr{\"a}sentieren. Es wird argumentiert, da{\ss} es gerade dieses Merkmal ist, welches totalitaristische Staatsformen in erster Linie von Diktaturen und Tyrannei unterscheidet, die durch die unkontrollierte Machtausiibung nach freiem Ermessen des Tyrannen charakterisiert sind. Schlie{\ss}lich werden notwendige Elemente der Verfassung des Totalitarismus herausgearbeitet.},
 author = {Bernholz, Peter},
 year = {1991},
 title = {{T}he {C}onstitution of {T}otalitarianism},
 url = {\url{http://www.jstor.org/stable/40751427}},
 pages = {425--440},
 volume = {147},
 number = {3},
 issn = {09324569},
 journal = {Journal of Institutional and Theoretical Economics (JITE) / Zeitschrift f{\"u}r die gesamte Staatswissenschaft}
}


