@Misc{Grimm.09.12.09.2010,
  Title                    = {{I}nstitutionalizing {H}ybrid {R}egimes in {B}osnia-{H}erzegovina and {K}osovo},

  Author                   = {Grimm, Sonja},
  Year                     = {09.-12.09.2010},

  Abstract                 = {The international community of states has put much emphasis on institutionalizing stateinstitutions and democracy in Bosnia-Herzegovina and Kosovo for more than a decade now. However, both political interventions effected the consolidation of hybrid regimes with strong authoritarian elements on the ground, but not the consolidation of democracy. This paradox result of democracy promotion is in particular a problem for the European Union (EU). From the beginning the EU was urged to supervise in both countries a fragile peace process. This involvement bears a challenge: As a regional institution, foremost interested in well-developed economic relationships, the EU seeks the implementation of economic competition and political contestation. As a benevolent (and permanently interfering) supervisor of the peace process, foremost interested in regional security, the EU seeks stabilization of the political system and predictability of the target state's behaviour. As an unintended consequence, the EU thereby supports the development of two hybrid regimes with severe authoritarian deficits. That outcome can be explained by the fact that the EU is hardly able to perform two opposing roles at once that differ substantially in their goals and accordingly in their strategies to influence the preferences of the target elites. The two roles build on opposing logics of influencing domestic reform-making from the outside and thereby produce contradicting results.},
  Address                  = {Stockholm},
  Series                   = {7th {P}an-{E}uropean {C}onference on {I}nternational {R}elations}
}

