@misc{Mller.2011,
 abstract = {The impact of structural factors on the political regime type has received much attention in the latest decades. However, cross-temporal changes in the relationships have remained largely unexplored. In this paper, we examine several hypotheses about the diachronical dynamics of such structural conditions on autocracy using a sliding series of statistical analyses, covering 167 countries from 1973 to 2007. First, we propose that -- due to the advent of liberal hegemony following the end of the Cold War -- structural factors generally do not account for the presence of autocracies as much today as what was the case at recently as in the 1980s. Our empirical analysis shows that this proposition can only be corroborated in part. On this basis, we argue that liberal hegemony has had an asymmetrical impact on the structural conditions of autocracy. More particularly, while it is likely to have diminished the effect of Neighbor diffusion and Modernization, we expect the variables of being oil reliant and having a Muslim majority to be largely resistant to the impact of liberal hegemony. This assertion finds more empirical backing.},
 author = {M{\o}ller, J{\o}rgen and Muno, Wolfgang and Skaaning, Svend-Erik},
 year = {2011},
 title = {{S}tructural {D}eterminants of {A}utocracy during the {T}hird {W}ave: {P}aper f{\"u}r die {A}utorenkonferenz {P}{V}{S}-{S}onderheft 2012 {\glqq}{A}utokratien im {V}ergleich{\grqq}},
